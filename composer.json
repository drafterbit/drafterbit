{
  "name": "drafterbit/drafterbit",
  "description": "Web app you can use to create a website",
  "keywords": [
    "website", "blog", "cms"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "egig",
      "email": "egigundari@gmail.com"
    }
  ],
 "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/drafterbit/framework.git"
      },
      {
          "type": "vcs",
          "url": "https://github.com/egig/tika.git"
      }
  ],
  "require": {
      "drafterbit/framework": "dev-master",
      "symfony/config": "2.5.*",
      "symfony/translation": "2.5.*",
      "symfony/filesystem": "2.5.*",
      "symfony/finder": "2.5.*",
      "twig/twig": "1.15.*",
      "ircmaxell/password-compat": "1.0.*",
      "natxet/CssMin": "3.0.2",
      "mrclay/minify": "2.2.*",
      "gregwar/image": "2.0.*",
      "swiftmailer/swiftmailer": "5.2.*"
  },
  "config": {
    "vendor-dir": "system/vendor",
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
  }
}