<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">{{ __('Info') }}</h3>
    </div>
    <div class="panel-body row">
        {% for k,v in info %}
            <div class="col-md-6">
                <div><strong>{{ __(k) }} </strong> {{ v }}</div>
            </div>
        {% endfor %}
    </div>
</div>